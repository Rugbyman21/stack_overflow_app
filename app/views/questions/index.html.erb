<div class="container">
  <br>
    <h1>Questions</h1>
    <div class ="col-md-10 nopadding">
      <ul id = "questions_list_ul">
        <% @questions.each do |question| %>
        <li><%= link_to question.statement, question_path(question) %></li>
        <% end %>
      </ul>
      <%= link_to "Add New Question", new_question_path %>
    </div>
</div>
